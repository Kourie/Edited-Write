h1 {
	color:white;
	font-size:30px;
	text-align:center;
}

h4 {
  color:white;
	font-size:23px;
  text-align:center;
}

h2 {
	color:white;
	font-size:30px;
	text-align:center;
}

h3 {
	color:white;
	font-size:23px;

}

table {
  font-family: Arial, Garamond, sans-serif, cursive;
  border-collapse: collapse;
  width: 100%;

}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;

}

tr:nth-child(even) {

  background-color: #b1b1b1;

}




.nav {
  overflow: hidden;
  background-color: #64585f;
  text-align: center;
  transition-duration: 0.4s;
}

.nav a {
  float: center;
  color: #f2f2f2;
  text-align: center;
  padding: 3px 120px;
  text-decoration: none;
  font-size: 22px;
  transition-duration: 0.4s;
}

.nav a:hover {
  background-color: #ddd;
  color: black;
  text-align: center;
}

html, body 
{
  margin: 0;
}
/* the Nav bar is above. hover and everything works*/



